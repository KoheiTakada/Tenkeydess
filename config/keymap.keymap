#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/pointing.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        MacLayer {
            bindings = <
&lt 3 Q           &kp W     &kp E                 &kp R    &kp T     &kp ESCAPE                  &kp Y      &kp U    &kp I           &kp O                 &kp P
&mt LEFT_SHIFT A  &kp S     &kp D                 &kp F    &kp G     &kp TAB                     &kp H      &kp J    &kp K           &kp L                 &kp MINUS
&lt 2 Z           &kp X     &kp C                 &kp V    &kp B     &none              &none    &kp N      &kp M    &kp COMMA       &kp DOT               &kp FSLH
&kp LGUI          &kp LALT  &mt LCTRL LANGUAGE_1  &kp DEL  &kp BSPC  &lt 5 EXCLAMATION  &lt 5 A  &kp SPACE  &kp RET  &kp LANGUAGE_2  &kp LC(LS(NUMBER_4))  &kp SEMICOLON
            >;
        };

        WindowsLayer {
            bindings = <
&trans  &trans  &trans  &trans  &trans  &trans          &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans          &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };

        NumberLayer {
            bindings = <
&trans  &trans  &trans  &trans  &trans  &trans          &kp N7  &kp N8   &kp N9  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans          &kp N4  &kp N5   &kp N6  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &kp N1  &kp N2   &kp N3  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &kp N0  &kp DOT  &trans  &trans  &trans
            >;
        };

        BracketsAndArrows {
            bindings = <
&trans  &trans  &trans  &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &trans          &trans          &kp UP_ARROW    &trans           &trans  &trans
&trans  &trans  &trans  &kp LEFT_BRACKET      &kp RIGHT_BRACKET      &trans          &kp LEFT_ARROW  &kp DOWN_ARROW  &kp RIGHT_ARROW  &trans  &trans
&trans  &trans  &trans  &trans                &trans                 &trans  &trans  &trans          &trans          &trans           &trans  &trans
&trans  &trans  &trans  &trans                &trans                 &trans  &trans  &trans          &trans          &trans           &trans  &trans
            >;
        };

        ScrollLayer {
            bindings = <
&trans  &trans  &trans  &trans  &trans  &trans          &mkp MB4   &mkp MB5   &trans    &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans          &mkp LCLK  &mkp RCLK  &mkp MB3  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans     &trans     &trans    &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans     &trans     &trans    &trans  &trans
            >;
        };

        BlootoothLayer {
            bindings = <
&bt BT_SEL 1  &trans  &trans  &trans  &trans  &trans          &trans  &trans  &trans  &trans  &trans
&trans        &trans  &trans  &trans  &trans  &trans          &trans  &trans  &trans  &trans  &trans
&trans        &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans        &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };
    };
};
